{
  "name": "VisualGit",
  "displayName": "VisualGit",
  "version": "1.0.0",
  "description": "A visually-oriented Git client",
  "main": "main.js",
  "engines": {
    "atom-shell": "1.2.3"
  },
  "scripts": {
    "start": "electron main.js",
    "compile": "webpack --progress --profile --colors --display-error-details --display-cached",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "package-win": "electron-packager ./ --overwrite --platform=win32 --arch=x64 --out=releases --icon=./assets/icons/logo.ico",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=releases --icon=./assets/icons/logo.icns",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=releases --icon=./assets/icons/logo.png",
    "lint": "tslint --project tsconfig.json",
    "postinstall": "typings install",
    "test": "jest"
  },
  "keywords": [
    "VisualGit",
    "visual",
    "git",
    "git client",
    "git workflow"
  ],
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "async": "2.1.4",
    "bootstrap": "3.3.6",
    "bootstrap-submenu": "2.0.4",
    "color-scheme": "0.0.5",
    "core-js": "2.4.0",
    "crypto-js": "^3.1.9-1",
    "electron-debug": "1.0.0",
    "es6-shim": "^0.35.1",
    "font-awesome": "4.7.0",
    "fs-extra": "^7.0.1",
    "fs-sync": "1.0.6",
    "github-avatar-url": "1.0.0",
    "github-username": "3.0.0",
    "jquery": "2.1.1",
    "jquery-contextmenu": "2.4.2",
    "material-letter-icons": "1.0.7",
    "nodegit": "0.13.2",
    "octonode": "0.7.7",
    "opn": "4.0.2",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "typescript-compiler": "^1.4.1-2",
    "typings": "1.3.0",
    "vis": "4.16.0",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.7",
    "@types/webpack-env": "^1.13.9",
    "angular2-template-loader": "^0.6.2",
    "css-loader": "^0.23.1",
    "devtron": "1.1.0",
    "electron-packager": "7.0.0",
    "electron-prebuilt": "1.2.8",
    "electron-reload": "^0.3.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "install": "^0.12.2",
    "jest": "^24.5.0",
    "jest-preset-angular": "^7.0.1",
    "json-loader": "^0.5.4",
    "npm": "^6.9.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.8",
    "ts-jest": "23.0.0",
    "ts-loader": "^0.8.2",
    "tslint": "^5.14.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "uglify-js": "^2.7.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "xo": "0.15.0"
  }
}